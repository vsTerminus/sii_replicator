	engine_nofuel[]: .enf3
	engine_nofuel[]: .enf4

	air_gear[0]: .airgear0
	air_gear[1]: .airgear1
	air_gear[2]: .airgear2

	air_brake[0]: .airbrake0
	air_brake[1]: .airbrake1
	air_brake[2]: .airbrake2
	air_brake[3]: .airbrake3

	engine_brake[]: .eb0

	horn: .horn
	air_horn: .ahorn
	reverse: .reverse

@include "/def/vehicle/truck/common_sound_ext.sui"

	suitable_for[]: "nhs275_275.%truck%.engine"
	suitable_for[]: "nhs275_550.%truck%.engine"
	suitable_for[]: "nhs275_675.%truck%.engine"
	suitable_for[]: "nhs275_825.%truck%.engine"
}

